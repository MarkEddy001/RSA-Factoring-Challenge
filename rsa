import math

def trial_division(n):
  """Returns the smallest prime factor of n, or None if n is prime."""
  for i in range(2, int(math.sqrt(n)) + 1):
    if n % i == 0:
      return i
  return None

def main():
  with open('tests/rsa-15') as f:
    n = int(f.read().strip())
    p = trial_division(n)
    if p is None:
      print('{} is prime.'.format(n))
    else:
      q = n // p
      print('{}={}*{}'.format(n, p, q))

if __name__ == '__main__':
  main()

