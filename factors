import math

def factors(n):
  """Returns a list of the prime factors of n."""
  factors = []
  while n > 1:
    for i in range(2, int(math.sqrt(n)) + 1):
      if n % i == 0:
        factors.append(i)
        n //= i
        break
    else:
      factors.append(n)
  return factors

def main():
  with open('tests/test00') as f:
    for line in f:
      n = int(line.strip())
      factors = factors(n)
      print('{}={}*{}'.format(n, factors[0], factors[1]))

if __name__ == '__main__':
  main()
